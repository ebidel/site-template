{
  "name": "site-template",
  "private": true,
  "version": "0.0.1",
  "description": "Generic Express/Node/App Engine static site server",
  "author": "Eric Bidelman <ebidel@>",
  "license": "Apache-2.0",
  "main": "server.mjs",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "disabled_postinstall": "gulp",
    "start": "node --experimental-modules server.mjs",
    "deploy": "npm run build && gcloud app deploy app.yaml",
    "lint": "./node_modules/eslint/bin/eslint.js --ext .js,.mjs .",
    "bundle": "rollup -c",
    "build": "gulp && npm run bundle"
  },
  "devDependencies": {
    "cssnano": "^4.1.0",
    "del": "^3.0.0",
    "eslint": "^5.5.0",
    "eslint-config-google": "^0.10.0",
    "google-closure-compiler": "^20180910.0.0",
    "gulp": "^3.9.1",
    "gulp-fn": "^0.0.3",
    "gulp-gray-matter": "^3.0.1",
    "gulp-load-plugins": "^1.2.4",
    "gulp-rename": "^1.2.2",
    "gulp-webp": "^3.0.0",
    "merge-stream": "^1.0.0",
    "postcss": "^7.0.2",
    "postcss-css-variables": "^0.10.0",
    "postcss-import": "^12.0.0",
    "rollup": "^0.66.2",
    "rollup-plugin-filesize": "^4.0.1",
    "rollup-plugin-includepaths": "^0.2.3",
    "rollup-plugin-terser": "^3.0.0",
    "run-sequence": "^2.2.1",
    "uglify-js": "^3.4.9"
  },
  "dependencies": {
    "date-fns": "^1.29.0",
    "express": "^4.16.3",
    "gray-matter": "^4.0.1",
    "highlight.js": "^9.12.0",
    "marked": "^0.5.0",
    "node-fetch": "1.7.3",
    "nunjucks": "^3.1.3",
    "nunjucks-markdown": "^2.0.1",
    "recursive-readdir": "^2.2.2"
  }
}
